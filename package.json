{
  "name": "tvmc-database",
  "version": "1.0.0",
  "type": "module",
  "description": "This application pulls and refactors the data from the Vorne XL database.",
  "main": "index.js",
  "scripts": {
    "test": "node index.js",
    "init-myini": "node src/init-myini.js",
    "init-db": "cd mysql/bin && mysqld --initialize-insecure",
    "init-password": "node src/init-password.js",
    "init-database": "node src/init-database.js",
    "init-table": "node src/init-table.js",
    "init": "npm-run-all -s init-myini init-db -p start-db -s init-password init-database init-table",
    "start-db": "cd mysql/bin && start /B mysqld",
    "query-loop": "node src/recurrance.js",
    "query-solo": "node modules/query.js"
  },
  "author": "Stephen Chryn",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "fs": "^0.0.1-security",
    "js-yaml": "^4.1.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "readline-sync": "^1.4.10",
    "url": "^0.11.1",
    "yargs": "^17.7.2"
  }
}
