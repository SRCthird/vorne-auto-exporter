{
  "name": "tvmc-database",
  "version": "1.0.0",
  "type": "module",
  "description": "This application pulls and refactors the data from the Vorne XL database.",
  "main": "index.js",
  "scripts": {
    "test": "node index.js",
    "myini": "node src/initdb.js",
    "initialize-db": "cd mysql/bin && mysqld --initialize-insecure",
    "generate": "node src/generate.js",
    "init": "npm-run-all -s myini initialize-db -p start-db generate",
    "start-db": "cd mysql/bin && mysqld",
    "start-loop": "node src/query.js"
  },
  "author": "Stephen Chryn",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "fs": "^0.0.1-security",
    "js-yaml": "^4.1.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "url": "^0.11.1"
  }
}